<div class="card">
  <div class="card-block">
    <h4 class="card-title">
      <%= link_to series.name, series %>
    </h4>
    <p class="card-text">
      <%= series.description %>
    </p>
    <div class="card-block">
      <% @plaques = series.plaques.photographed.limit(5) %>
      <%= render partial: 'plaques/tiles' %>
    </div>
  </div>
</div>
